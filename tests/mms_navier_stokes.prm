# Listing of Parameters
# ---------------------
subsection meta
    set dim = 2
end

subsection geometry
    set grid_name = hyper_ball
end

subsection initial_values
    #set Function expression = -y^3; x^3; 0.; 0.
    set Function expression = 0.; 0.; 0.; 0.
end

subsection refinement
    set initial_global_cycles = 2
end

subsection nonlinear_solver
    set max_iterations = 50
    set tolerance = 1.e-8
end

subsection time
    set end = 0.
    set initial_step_size = 0.
    set min_step_size = 0.
    set max_step_size = 0.
end

subsection output
    set write_solution_vtk = true
end
