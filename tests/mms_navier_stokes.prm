# Listing of Parameters
# ---------------------
subsection meta
    set dim = 2
end

subsection geometry
    set grid_name = hyper_shell
    set sizes = 0.1, 0.5
end

subsection initial_values
    set Function constants = r0 = 0.1
    set Function expression = -(sqrt(x^2 + y^2) - r0)*sin(atan2(y, x)); (sqrt(x^2 + y^2) - r0)*cos(atan2(y, x)); 0.; 0.
end

subsection refinement
    set initial_global_cycles = 2
end

subsection nonlinear_solver
    set max_iterations = 50
    set tolerance = 1.e-8
end

subsection time
    set end = 0.
    set initial_step_size = 0.
    set min_step_size = 0.
    set max_step_size = 0.
end

subsection output
    set write_solution_vtk = true
end
